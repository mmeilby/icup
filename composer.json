{
    "name": "vendor/icup",
    "license": "MIT",
    "type": "project",
    "description": "Match tracking system for sport events",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "authors": [
        {
            "name": "mm",
            "email": "mm@iserve.dk"
        }
    ],
    "require": {
        "php": ">=5.3.7",
        "symfony/symfony": "~2.5",
        "jackalope/jackalope-doctrine-dbal": "1.1.*",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle": "~1.2",
        "doctrine/data-fixtures": "1.0.*",
        "doctrine/doctrine-cache-bundle": "1.0.*",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "symfony/translation": "~2.3",
        "symfony-cmf/symfony-cmf": "1.2.*",
        "symfony-cmf/block-bundle": "1.2.*",
        "symfony-cmf/simple-cms-bundle": "1.2.*",
        "symfony-cmf/media-bundle": "1.2.*",
        "symfony-cmf/create-bundle": "1.2.*",
        "sensio/distribution-bundle": "~3.0",
        "sensio/framework-extra-bundle": "~3.0",
        "incenteev/composer-parameter-handler": "~2.0",
        "symfony/intl": "~2.3",
        "jms/security-extra-bundle": "1.5.*",
        "ircmaxell/password-compat": "1.0.*",
        "friendsofsymfony/jsrouting-bundle": "1.5.*",
        "lunetics/locale-bundle": "2.2.*",
        "nelmio/alice": "1.*",
        "liip/imagine-bundle": "dev-master",
        "sonata-project/core-bundle": "~2.2",
        "sonata-project/admin-bundle": "2.4.x-dev",
        "kartik-v/bootstrap-fileinput": "dev-master"
    },
    "require-dev": {
        "liip/functional-test-bundle": "1.0.*",
        "sensio/generator-bundle": "~2.3"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Symfony\\Cmf\\Bundle\\CreateBundle\\Composer\\ScriptHandler::downloadCreateAndCkeditor"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Symfony\\Cmf\\Bundle\\CreateBundle\\Composer\\ScriptHandler::downloadCreateAndCkeditor"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/local/parameters.yml",
            "dist-file": "app/config/parameters.yml.dist",
            "env-map": {
            }
        },
        "branch-alias": {
            "dev-master": "2.3-dev"
        }
    }
}
